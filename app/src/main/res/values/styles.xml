<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="menu_labels_style">
        <item name="android:background">#38000000</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="fab_delete_style">
        <item name="android:icon">@drawable/delete_icon</item>
    </style>

    <style name="item_text_style">
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">casual</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="datePicker_style">
        <item name="android:calendarTextColor">@color/white</item>
        <item name="android:yearListSelectorColor">#45A3BA</item>
        <item name="android:headerBackground">#256F81</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:scaleX">1.0</item>
        <item name="android:scaleY">1.0</item>
    </style>
</resources>